          "decisionStrategy" : "AFFIRMATIVE",
          "config" : {
            "resources" : "[\"Users\"]",
            "scopes" : "[\"view\"]",
            "applyPolicies" : "[\"admin group\"]"
          }
        } ],
        "scopes" : [
          { "name" : "manage" },
          { "name" : "view"},
          { "name" : "map-roles" },
          { "name" : "impersonate" },
          { "name" : "user-impersonated" },
          { "name" : "manage-group-membership" }
